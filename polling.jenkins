pipeline {
  agent any

  triggers {
    pollSCM "* * * * *"
  }

  options {
    timestamps()
  }

  stages {

    stage ('Clone') {
      steps {
        echo '[LOG] started cloning repo'
        dir ('s') {
          git url: 'https://github.com/topalach/ravendb.git', branch: 'v4.0-jenkins-prtest10', poll: true
        }
        echo '[LOG] finished cloning repo'
      }
    }

  }
}